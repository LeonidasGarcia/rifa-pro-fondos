{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://server-node-neon-rho.vercel.app/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}